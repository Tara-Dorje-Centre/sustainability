<?php 

class _entityTypeList extends _entityList{
	protected $__MyClassName = '__entityTypeList';
	protected $_titleEntity = 'Entity Types';
	public $base;
	
	public function __construct($mode = 'VIEW', $id = 0, $idParent = 0){
		parent::__construct($mode, $id, $idParent);
		$this->base = new _entityType('LIST');
	}
	
	protected function setPageMenu(){
		$menuType = 'LIST';
		$menuStyle = 'menu';
		$this->base->links->saveMenuSettings();
		$this->base->links->setType($menuType);
		$this->base->links->setStyle($menuStyle);
		
		
		$menuL = new MenuLinks($menuType,$menuStyle);
			
		$menu = $this->base->links->openMenu('section-heading-links');
		$menu .= $menuL->linkReference();
		$menu .= $this->base->links->resetMenu();
		$menu .= $this->base->links->detailAddHref();
		$menu .= $this->base->links->listingHref();		
		$menu .= $this->base->links->closeMenu();
		$this->_pageMenu = $menu;
		
		$this->base->links->restoreMenuSettings();
	}
	
	
}



?>
