<?php 

class MeasureSQL extends _taskChildSQL{

public $baseTable = 'measures';
protected function cols(){
$c = $this->select();
$c = " m.id, ";
$c .= " m.task_id, ";
$c .= " m.location_id, ";
$c .= " m.name, ";
$c .= " m.description, ";
$c .= " m.date_reported, ";
$c .= " m.updated, ";
$c .= " m.value, ";
$c .= " m.measure_type_unit_id, ";
$c .= " m.notes, ";
$c .= " mtu.measure_type, ";
$c .= " mtu.unit_type, ";
$c .= " mtu.unit_symbol ";
return $c;	
}

protected function tables($joinTypes = true){
	$t = " FROM measures AS m JOIN measure_type_units_v as mtu ";
	$t .= " ON m.measure_type_unit_id = mtu.measure_type_unit_id ";
	return $t;
}


}
?>
