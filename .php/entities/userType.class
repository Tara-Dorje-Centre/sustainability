class userType extends _entityType {
	public function __construct($mode = 'VIEW', $id = 0, $idParent = 0){
		$this->_MyClassName = 'userType';
		parent::__construct($mode, $id, $idParent);
		
		$this->entity = 'user-type';
  		$this->titleEntity = 'User Profile Type';
		$this->pageSave = 'sys_UserType_Save.php';
		
		$this->f = new userTypeFields();
		$this->f->setEntity($this->entity);
		$this->sql = new userTypeSQL();
		$this->links = new userTypeLinks();
	}
} 
class userTypeList extends _entityTypeList{
	public function __construct($mode = 'VIEW', $id = 0, $idParent = 0){
		$this->_MyClassName = 'userTypeList';
		parent::__construct($mode, $id, $idParent);
		
  		$this->titleEntity = 'User Profile Types';
		$this->base = new userType($this->mode,$this->id,$this->idParent);
	}
}
