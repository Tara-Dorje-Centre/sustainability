<?php
namespace application\entities\projects\links;

trait taskChildParameters {
use taskParameters;
	protected function setTaskChild(\application\links\url &$url, $id){
		$url->addParameter($this->pTaskId($id));
	}
	protected function pTaskId($idTask){
		return $this->getParameter('task-id',$idTask);
	}
	protected function pApprovedTask($approved){
		return $this->getParameter('approved-task',$approved);
	}
}



?>
