<?php
namespace application\entities\projects\links;

trait activityChildParameters{
use activityParameters;
	protected function setActivityChild(\application\links\url &$url, $id){
		$url->addParameter($this->pActivityId($id));
	}
	protected function pActivityId($id){
		return $this->getParameter('activity-id',$id);
	}
	protected function pApproved($approved){
		return $this->getParameter('approved',$approved);
	}
}


?>
