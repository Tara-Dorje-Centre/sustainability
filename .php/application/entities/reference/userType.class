<?php
namespace application\entities\reference;

class userTypeFields extends \application\forms\entityTypeFields{

}
class userTypeSQL extends \application\sql\entityTypeSQL{
//missing display order from table?
	public function __construct(){
		$this->baseTable = 'user_types';
	}
}
class userTypeLinks extends \application\links\entityTypeLinks{
	protected $context = 'userType';
	protected $pageList = 'userType.php';
	protected $pageDetail = 'userType.php';
}
class userType extends \application\entityType {
	public function __construct($mode = 'VIEW', $id = 0, $idParent = 0){
		$this->_MyClassName = 'userType';
		parent::__construct($mode, $id, $idParent);
		
		$this->entity = 'user-type';
  		$this->titleEntity = 'User Profile Type';
		$this->pageSave = 'userTypeSave.php';
		
		$this->f = new userTypeFields();
		$this->f->setEntity($this->entity);
		$this->sql = new userTypeSQL();
		$this->links = new userTypeLinks();
	}
} 
class userTypeList extends \application\entityTypeList{
	public function __construct($mode = 'LIST', $id = 0, $idParent = 0){
		$this->_MyClassName = 'userTypeList';
		parent::__construct($mode, $id, $idParent);
		
  		$this->titleEntity = 'User Profile Types';
		$this->base = new userType($this->mode,$this->id,$this->idParent);
	}
}
