<?php
namespace application\entities\reference;



class locationType extends \application\entityType{
	public function __construct($mode = 'VIEW', $id = 0, $idParent = 0){
		$this->_MyClassName = 'locationType';
		parent::__construct($mode, $id, $idParent);
		$this->entity = 'location-type';
  	 	$this->titleEntity = 'Location Type';
	 	$this->pageSave = 'pr_LocationType_Save.php';
		$this->f = new locationTypeFields();
		$this->sql = new locationTypeSQL();
		$this->links = new locationTypeLinks();
		$this->f->setEntity($this->entity);
	}
	
} 
class locationTypeList extends \application\entityTypeList{

 	public function __construct($mode = 'VIEW', $id = 0, $idParent = 0){
 		$this->_MyClassName = 'locationTypeList';
		parent::__construct($mode, $id, $idParent);

	 	$this->titleEntity = 'Location Types';
		$this->base = new locationType($this->pageMode,$this->id,$this->idParent);
	}
	
}
