<?php
namespace application\entities\reference;

class receiptTypeFields extends \application\forms\entityTypeFields{

}
class receiptTypeSQL extends \application\sql\entityTypeSQL{
	public function __construct(){
		$this->baseTable = 'receipt_types';
	}
}
class receiptTypeLinks extends \application\links\entityTypeLinks{
	protected $context = 'receiptType';
	protected $pageList = 'pr_ReceiptType_List.php';
	protected $pageDetail = 'pr_ReceiptType_Detail.php';
}
class receiptType extends \application\entityType{

 	public function __construct($mode = 'VIEW', $id = 0, $idParent = 0){
		$this->_MyClassName = 'receiptType';
		parent::__construct($mode, $id, $idParent);
	
		$this->entity = 'receipt-type';
  		$this->titleEntity = 'Receipt Type';
  		$this->pageSave = 'pr_ReceiptType_Save.php';
		
		$this->f = new receiptTypeFields();
		$this->f->setEntity($this->entity);
		$this->sql = new receiptTypeSQL;
		$this->links = new receiptTypeLinks();
	}
} 
class receiptTypeList extends \application\entityTypeList{
 	public function __construct($mode = 'VIEW', $id = 0, $idParent = 0){
		$this->_MyClassName = 'ReceiptTypeList';
		parent::__construct($mode, $id, $idParent);
		
		$this->titleEntity = 'Receipt Types';
		$this->base = new receiptType($this->pageMode,$this->id,$this->idParent);
	}
}

