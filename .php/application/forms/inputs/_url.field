<?php 
namespace application\forms;

class _url extends _text{

	public function input( ){
		$i = new \html\inputs\url($this->env->variable(),$this->value(),$this->_caption);
		$i->setTooltip($this->_tooltip);
		$i->setValidation($this->_validation);
		return $i->print();
	}
	
	public function inputNoCaption( ){
 		$i = new \html\inputs\url($this->env->variable(),$this->value(),$this->_caption);
		return $i->printNoCaption();
	}
	
}


?>
