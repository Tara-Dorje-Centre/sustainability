namespace html;

class _inputUser extends _inputName{
	public function __construct($name, $value, $tooltip = 'none', $css = 'editing-input-user'){
		parent::__construct($name,$value, $tooltip,$css);
		//setDefaultDimenions
		$this->setDimensions(50, 8);
	}
}

function inputFieldUser(
	$entity,
	$value,
	$id = 'user',
	$caption = 'User',
	$tooltip = NULL, 
	
	$css = 'editing-input-user'){
		
	$element = inputFieldText($entity,$value,$id,$caption,8,50,$tooltip,$disabled,$css);
	return $element;
	
}
