<?php
namespace html;

class user extends name{
	public function __construct($name, $value, $caption,$tooltip = 'Enter User', $css = 'editing-input-user'){
		parent::__construct($name,$value, $caption,$tooltip,$css);
		
	}
}

function inputFieldUser(
	$entity,
	$value,
	$id = 'user',
	$caption = 'User'){
		
	$element = new user($id,$value,$caption);
	return $element->print();
	
}
