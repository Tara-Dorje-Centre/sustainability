<?php
namespace html;

class password extends _input{
	public function __construct($name = 'password', $caption = 'Password',$tooltip = 'Enter Password'){
		parent::__construct('password',$name,$tooltip,'editing-input-password');
		$this->setSize(10,50);
		$this->setCaption($caption);
	}
}

function getPasswordInput($n,$c){
	$e = new password($n,$c);
	return $e->print();
}


?>
