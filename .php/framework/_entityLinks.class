<?php

abstract class _entityLinks extends _baseEntityLinks {

	public function listingPaged($idParent, $found, $resultPage=1, $perPage=10){
		return $this->pagingLinksChildren($idParent, $found,$resultPage,$perPage);
	}
	
	public function listingHref($idParent = -1, $caption = 'List'){
	return $this->list($this->context.$caption,$idParent);
	}	

	
}


?>
