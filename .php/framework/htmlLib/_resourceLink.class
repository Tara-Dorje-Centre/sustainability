<?php
namespace html;

class _inlineStyle extends _anyElement{
	public function __construct(){
		parent::__construct('script');
	}
}

class _resourceLink extends _anyEmptyElement{
	public function __construct($rel,$type,$href){
		parent::__construct('link');
		$this->makeAttribute('rel',$rel);
		$this->makeAttribute('type',$type);
		$this->makeAttribute('href',$href);
	}
	
}

class _cssLink extends _resourceLink{
	public function __construct($cssFile){
		parent::__construct('stylesheet','text/css',$cssFile);
	}
}

?>
