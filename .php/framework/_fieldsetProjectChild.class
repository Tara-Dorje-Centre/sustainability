<?php 
interface IfieldsetProjectChild{
	public $project;
	public $location;
	public $started;
	public $approved;
	public $approvedBy;

}

abstract class _fieldsetProjectChild extends _fieldsetChild
implements  IfieldsetProjectChild, IfieldsetChild, Ifieldset, Iecho, IcontentWriter{

	public $project;
	public $location;
	public $started;
	public $approved;
	public $approvedBy;
    public function __construct(){
    	parent::__construct();
    	$this->project = new _idNameField('project_id','idProject','project_name','Project',true);
    	$this->location = new _idNameField('location_id','idLocation','location_name','Location',true);
    	$this->started = new _dateField('started','started','Started',true);           
    	$this->approved = new _dateField('approved','approved','Approved',true);
    	$this->approvedBy = new _textField('approved_by','approvedBy','Approved By',true);
    }
    protected function reset(){
    	parent::reset(){
    	$this->project->reset(0);
    	$this->location->reset(0);
    	$this->started->reset('');
    	$this->approved->reset('');
    	$this->approvedBy->reset('n/a');
    	
    }
	public function setRow($row){
		parent::setRow($row);
		$this->project->fetch($row);
		$this->location->fetch($row);
		$this->started->fetch($row);
    	$this->approved->fetch($row);
    	$this->approvedBy->fetch($row);
	}
	public function format(){
		parent::format();
		$this->project->format();
		$this->location->format();
		$this->started->format();
    	$this->approved->format();
    	$this->approvedBy->format();
	}
	public function display(){;
		$this->addContent(parent::display(),true);
		$this->addContent($this->location->p());		
		$this->addContent($this->project->p());		
		$this->addContent($this->started->p());		
		$this->addContent($this->approved->p());		
		$this->addContent($this->approvedBy->p());		
		return $this->getContent();
	}
	public function listHeading(){
		$this->addContent(parent::listHeading(),true);
		$this->addContent($this->location->th());		
		$this->addContent($this->project->th());		
		$this->addContent($this->started->th());		
		$this->addContent($this->approved->th());		
		$this->addContent($this->approvedBy->th());		
		return $this->getContent();
	}
	public function listDetail(){
		$this->addContent(parent::listDetail(),true);
		$this->addContent($this->location->td());
		$this->addContent($this->project->td());
		$this->addContent($this->started->td());		
		$this->addContent($this->approved->td());		
		$this->addContent($this->approvedBy->td());		
		return $this->getContent();
	}
	public function getPostVars(){
		parent::getPostVars();
		$this->project->post();
		$this->location->post();
		$this->started->post();
    	$this->approved->post();
    	$this->approvedBy->post();
	}
}


?>
