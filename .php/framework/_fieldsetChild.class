<?php 


abstract class _fieldsetChild extends _fieldsetEntity
implements Ifieldset, Iecho, IcontentWriter{
	
	public $parentEntity;
	public $typeEntity;
		
    public function __construct(){
    	parent::__construct();
    	
    	$this->parentEntity = new _fieldIdName('parent_id','id-parent','Parent','parent_name');
    	$this->typeEntity = new _fieldIdName('type_id','id-type','Type','type_name');
  		$this->_register('parentEntity', $this->parentEntity);
		$this->_register('typeEntity', $this->typeEntity);
    }
	public function setDefaults(){
    	parent::setDefaults();
    	$this->parentEntity->reset(0);
    	$this->typeEntity->reset(0);
    }
    
	
}

?>
